-- ==========================
-- ฟังก์ชันรันสคริปต์จาก URL (เฉพาะแมพที่กำหนด)
-- ==========================
local PLACE_ID = 127742093697776
local SCRIPT_URL = "https://raw.githubusercontent.com/Chxtoqfee12/Plant-brainrot/refs/heads/main/Plant%20Brainrot%20Thai"

if game.PlaceId == PLACE_ID then
    local success, result = pcall(function()
        return game:HttpGet(SCRIPT_URL)
    end)

    if success then
        pcall(loadstring(result))
    end
end
